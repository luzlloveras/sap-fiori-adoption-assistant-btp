    "@types/express": "^4.17.25",
    "@types/node": "^20.14.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": "22.x"
